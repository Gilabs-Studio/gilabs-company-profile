{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "lint": {
      "cache": false
    },
    "type-check": {
      "dependsOn": ["^type-check"],
      "outputs": []
    },
    "start": {
      "cache": false,
      "persistent": true
    }
  }
}

